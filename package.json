{
  "name": "@mohammad_obed/react-native-common",
  "version": "0.3.5",
  "description": "a library that contains common react native and react and typescript code",
  "source": "src/index.tsx",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "./lib/typescript/module/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    },
    "./components": {
      "import": "./lib/module/components/index.js",
      "require": "./lib/commonjs/components/index.js",
      "types": "./lib/typescript/module/components/index.d.ts"
    },
    "./constants": {
      "import": "./lib/module/constants/index.js",
      "require": "./lib/commonjs/constants/index.js",
      "types": "./lib/typescript/module/constants/index.d.ts"
    },
    "./contexts": {
      "import": "./lib/module/contexts/index.js",
      "require": "./lib/commonjs/contexts/index.js",
      "types": "./lib/typescript/module/contexts/index.d.ts"
    },
    "./dev": {
      "import": "./lib/module/dev/index.js",
      "require": "./lib/commonjs/dev/index.js",
      "types": "./lib/typescript/module/dev/index.d.ts"
    },
    "./extensions": {
      "import": "./lib/module/extensions/index.js",
      "require": "./lib/commonjs/extensions/index.js",
      "types": "./lib/typescript/module/extensions/index.d.ts"
    },
    "./helpers": {
      "import": "./lib/module/helpers/index.js",
      "require": "./lib/commonjs/helpers/index.js",
      "types": "./lib/typescript/module/helpers/index.d.ts"
    },
    "./hooks": {
      "import": "./lib/module/hooks/index.js",
      "require": "./lib/commonjs/hooks/index.js",
      "types": "./lib/typescript/module/hooks/index.d.ts"
    },
    "./libs/Bugs": {
      "import": "./lib/module/libs/Bugs/index.js",
      "require": "./lib/commonjs/libs/Bugs/index.js",
      "types": "./lib/typescript/module/libs/Bugs/index.d.ts"
    },
    "./models": {
      "import": "./lib/module/models/index.js",
      "require": "./lib/commonjs/models/index.js",
      "types": "./lib/typescript/module/models/index.d.ts"
    },
    "./styles": {
      "import": "./lib/module/styles/index.js",
      "require": "./lib/commonjs/styles/index.js",
      "types": "./lib/typescript/module/styles/index.d.ts"
    },
    "./utils": {
      "import": "./lib/module/utils/index.js",
      "require": "./lib/commonjs/utils/index.js",
      "types": "./lib/typescript/module/utils/index.d.ts"
    },
    "./types/components": {
      "import": "./lib/module/types/components/index.js",
      "require": "./lib/commonjs/types/components/index.js",
      "types": "./lib/typescript/module/types/components/index.d.ts"
    },
    "./types/constants": {
      "import": "./lib/module/types/constants/index.js",
      "require": "./lib/commonjs/types/constants/index.js",
      "types": "./lib/typescript/module/types/constants/index.d.ts"
    },
    "./types/helpers": {
      "import": "./lib/module/types/helpers/index.js",
      "require": "./lib/commonjs/types/helpers/index.js",
      "types": "./lib/typescript/module/types/helpers/index.d.ts"
    },
    "./types/hooks": {
      "import": "./lib/module/types/hooks/index.js",
      "require": "./lib/commonjs/types/hooks/index.js",
      "types": "./lib/typescript/module/types/hooks/index.d.ts"
    },
    "./types/models": {
      "import": "./lib/module/types/models/index.js",
      "require": "./lib/commonjs/types/models/index.js",
      "types": "./lib/typescript/module/types/models/index.d.ts"
    },
    "./types/reducers": {
      "import": "./lib/module/types/reducers/index.js",
      "require": "./lib/commonjs/types/reducers/index.js",
      "types": "./lib/typescript/module/types/reducers/index.d.ts"
    },
    "./types/theme": {
      "import": "./lib/module/types/theme/index.js",
      "require": "./lib/commonjs/types/theme/index.js",
      "types": "./lib/typescript/module/types/theme/index.d.ts"
    }
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace react-native-common-example",
    "test": "jest",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean": "del-cli lib",
    "prepare": "bob build",
    "release": "release-it"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MohammadAObed/react-native-common.git"
  },
  "author": "MohammadAObed <mohammadahmadobed@gmail> (https://www.npmjs.com/~mohammad_obed)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MohammadAObed/react-native-common/issues"
  },
  "homepage": "https://github.com/MohammadAObed/react-native-common#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@evilmartians/lefthook": "^1.5.0",
    "@react-native-community/cli": "latest",
    "@react-native-community/slider": "4.5.6",
    "@react-native/eslint-config": "^0.73.1",
    "@release-it/conventional-changelog": "^9.0.2",
    "@tanstack/react-query": "^5.80.6",
    "@types/jest": "^29.5.5",
    "@types/react": "~19.0.10",
    "commitlint": "^17.0.2",
    "del-cli": "^5.1.0",
    "eslint": "^8.51.0",
    "expo": "^53.0.10",
    "expo-application": "~6.1.4",
    "expo-asset": "^11.0.4",
    "expo-constants": "^17.0.7",
    "expo-device": "~7.1.4",
    "expo-file-system": "^18.0.11",
    "expo-router": "^5.0.7",
    "expo-sqlite": "~15.2.11",
    "jest": "^29.7.0",
    "react": "19.0.0",
    "react-native": "0.79.3",
    "react-native-builder-bob": "^0.32.0",
    "react-native-element-dropdown": "^2.12.4",
    "react-native-error-boundary": "^1.2.7",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-heroicons": "^4.0.0",
    "react-native-paper": "^5.13.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-svg": "15.11.2",
    "react-native-toast-message": "^2.3.0",
    "react-native-wheel-color-picker": "^1.3.1",
    "release-it": "^17.10.0",
    "typescript": "~5.8.3"
  },
  "resolutions": {
    "@types/react": "^19.0.8"
  },
  "peerDependencies": {
    "@react-native-community/slider": "*",
    "@tanstack/react-query": "*",
    "@types/react": "*",
    "expo": "*",
    "expo-application": "*",
    "expo-asset": "*",
    "expo-constants": "*",
    "expo-device": "*",
    "expo-file-system": "*",
    "expo-router": "*",
    "expo-sqlite": "*",
    "react": "*",
    "react-native": "*",
    "react-native-element-dropdown": "*",
    "react-native-error-boundary": "*",
    "react-native-gesture-handler": "*",
    "react-native-heroicons": "*",
    "react-native-paper": "*",
    "react-native-reanimated": "*",
    "react-native-safe-area-context": "*",
    "react-native-svg": "*",
    "react-native-toast-message": "*"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@3.6.1",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off"
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "esm": true
        }
      ]
    ]
  },
  "create-react-native-library": {
    "languages": "js",
    "type": "library",
    "version": "0.47.0"
  },
  "dependencies": {
    "expo": "*"
  }
}
